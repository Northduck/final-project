{
	"compilerOptions": {
	"moduleResolution": "node",
	"outDir": "./dist",
	"target": "es6",
	"module": "es6",
	"jsx": "react",
	"allowJs": false,
    "esModuleInterop": true,
    "sourceMap": true,
    "strict": true,
    "noImplicitAny": true,
    "listEmittedFiles": true,
    "alwaysStrict": true,
    "incremental": false,
    "skipLibCheck": true,
    "checkJs": false,
	}
}